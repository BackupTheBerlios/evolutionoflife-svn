Imports System.ComponentModel



'=--------------------------------------------------------------------------=
' ImageButtonSizeMode
'=--------------------------------------------------------------------------=
' Controls how the image button control resizes itself when images are set.
' 
'
<LocalisableDescription("ImageButtonSizeMode")> _
Public Enum ImageButtonSizeMode

    <LocalisableDescription("ImageButtonSizeMode.AutoSize")> _
    AutoSize = 1

    <LocalisableDescription("ImageButtonSizeMode.StretchImage")> _
    StretchImage = 2

End Enum ' ImageButtonSizeMode



